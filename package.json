{
  "name": "altin-bot",
  "private": false,
  "version": "1.0.2",
  "description": "A customizable chatbot component for Vue.js applications",
  "type": "module",
  "author": "Altin Kelmendi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altink7/altin-bot.git"
  },
  "keywords": [
    "chatbot",
    "vue",
    "customizable",
    "UI"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/altin-bot.umd.cjs",
  "module": "./dist/altin-bot.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/altin-bot.js",
      "require": "./dist/altin-bot.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "path": "^0.12.7",
    "sass-embedded": "^1.80.4",
    "typescript": "~5.6.2",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.6"
  }
}
